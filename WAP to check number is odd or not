#include <stdio.h>

int main() {
    int number;

  
    printf("Enter an integer: ");

    // Read the integer from the user
    scanf("%d", &number);

  
    if (number % 2!= 0) {
        printf("%d is an odd number.\n", number);
    } else {
        printf("%d is not an odd number.\n", number);
    }

    return 0;
}
